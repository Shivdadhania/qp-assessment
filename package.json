{
  "name": "grocery-booking",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:create": "npm run typeorm migration:create ./src/migrations/$FOR",
    "migration:generate": "pnpm run typeorm migration:generate ./src/migrations/$FOR -d ./src/utils/database/connection/index.ts",
    "migration:up": "NODE_OPTIONS=--max-old-space-size=1024 ts-node ./node_modules/typeorm/cli.js migration:run -d ./src/utils/database/connection/index.ts",
    "migration:up-fake": "ts-node ./node_modules/typeorm/cli.js -d ./src/utils/database/connection/index.ts migration:run --fake",
    "migration:down": "ts-node ./node_modules/typeorm/cli.js migration:revert -d ./src/utils/database/connection/index.ts"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/typeorm": "^9.0.1",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "dotenv": "^16.4.5",
    "express-correlation-id": "^2.0.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.10"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/better-sqlite3": "^7.6.3",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/jest": "29.4.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/lodash": "^4.14.202",
    "@types/mocha": "^10.0.0",
    "@types/multer": "^1.4.11",
    "@types/node": "^18.11.9",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "axios-mock-adapter": "^1.22.0",
    "better-sqlite3": "^8.0.1",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.3.1",
    "jest-junit": "^15.0.0",
    "node-mocks-http": "^1.12.1",
    "pg-mem": "^2.6.4",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
